{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "polandcentral"
    },
    "sharedResourceGroupName": {
      "value": "RG-ALTO-Eureka-Crawler"
    },
    "environmentName": {
      "value": "env-eureka-crawler"
    },
    "uamiName": {
      "value": "uami-eureka-crawler"
    },
    "keyVaultName": {
      "value": "kv-eureka-XXXXX"
    },
    "acrName": {
      "value": "acrXXXXX"
    },
    "cosmosAccountName": {
      "value": "cosmos-eureka-XXXXX"
    },
    "jobBackfillName": {
      "value": "nsa-backfill"
    },
    "jobDeltaName": {
      "value": "nsa-delta"
    },
    "imageName": {
      "value": "nsa-crawler"
    },
    "imageTag": {
      "value": "latest"
    },
    "cpu": {
      "value": "2.0"
    },
    "memory": {
      "value": "4Gi"
    },
    "cronExpression": {
      "value": "10 5 * * *"
    },
    "proxyEnabled": {
      "value": true
    },
    "proxyListUrl": {
      "value": "YOUR-PROXY-LIST-URL"
    },
    "parallelSearchEnabled": {
      "value": true
    },
    "maxParallelSearchSessions": {
      "value": 10
    }
  }
}
